<effect LC_NUMERIC="C" id="CLIP_Math" tag="frei0r.select0r" type="customVideo" version="0.4">
 <name>CLIP_Math</name>
 <description>Chroma Key with more advanced options (e.g. different color models). Use if basic chroma key is not working effectively.</description>
 <author>Marko Cebokli</author>
 <parameter default="0x00ff00ff" name="Color to select" type="color" value="0=0x00ff00ff">
  <name>Color to select</name>
 </parameter>
 <parameter default="1" name="Invert selection" type="bool" value="1">
  <name>Invert selection</name>
 </parameter>
 <parameter default="0" name="Selection subspace" paramlist="0;0.5;1" type="list" value="0">
  <paramlistdisplay>RGB,ABI,HCI</paramlistdisplay>
  <name>Color Model</name>
 </parameter>
 <parameter default="0.5" name="Subspace shape" paramlist="0;0.5;1" type="list" value="0">
  <paramlistdisplay>Box,Ellipsoid,Diamond</paramlistdisplay>
  <name>Shape</name>
 </parameter>
 <parameter default="0.9" name="Edge mode" paramlist="0;0.35;0.6;0.7;0.9" type="list" value="0.9">
  <paramlistdisplay>Hard,Fat,Normal,Skinny,Slope</paramlistdisplay>
  <name>Edge mode</name>
 </parameter>
 <parameter default="0.2" factor="1000" max="1000" min="0" name="Delta R / A / Hue" type="animated" value="0=0.786">
  <name>Red / Hue Delta</name>
 </parameter>
 <parameter default="0.2" factor="1000" max="1000" min="0" name="Delta G / B / Chroma" type="animated" value="0=0.786">
  <name>Green / Chroma Delta</name>
 </parameter>
 <parameter default="0.2" factor="1000" max="1000" min="0" name="Delta B / I / I" type="animated" value="0=0.849">
  <name>Blue / Intensity Delta</name>
 </parameter>
 <parameter default="0" factor="1000" max="1000" min="0" name="Slope" type="animated" value="0=0.165">
  <name>Soften</name>
 </parameter>
 <parameter default="0.5" name="Operation" paramlist="0;0.3;0.5;0.7;1" type="list" value="0.5">
  <paramlistdisplay>Write on clear,Max,Min,Add,Subtract</paramlistdisplay>
  <name>Operation</name>
 </parameter>
</effect>
